{
  "name": "gdbots/crypto-bundle",
  "homepage": "https://github.com/gdbots/crypto-bundle-php",
  "description": "Symfony bundle that integrates defuse/php-encryption package.",
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "require": {
    "php": ">=8.2",
    "defuse/php-encryption": "^2.3",
    "symfony/console": "^6.4 || ^7.0",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "twig/twig": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\CryptoBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\CryptoBundle\\": "tests"
    }
  },
  "bin": [
    "bin/crypto-create-key"
  ],
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
