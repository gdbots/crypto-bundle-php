{
  "name": "gdbots/crypto-bundle",
  "homepage": "https://github.com/gdbots/crypto-bundle-php",
  "description": "Symfony bundle that integrates defuse/php-encryption package.",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "defuse/php-encryption": "^2.1",
    "symfony/console": "^3.3 || ^4.0",
    "symfony/framework-bundle": "^3.3 || ^4.0",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "composer/composer": "^1.5",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Gdbots\\Bundle\\CryptoBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gdbots\\Tests\\Bundle\\CryptoBundle\\": "tests"
    }
  },
  "bin": [
    "bin/crypto-create-key"
  ],
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
